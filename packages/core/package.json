{
  "name": "@builder/core",
  "version": "1.0.0",
  "description": "Builder core",
  "main": "build/index.js",
  "module": "build/es/index.js",
  "types": "build/es/index.d.ts",
  "scripts": {
    "start": "concurrently --kill-others \"npm run build:es -- --watch\" \"npm run build:lib -- --watch\" \"npm run server\"",
    "build": "npm run clean && npm run build:es && npm run build:lib",
    "build:prod": "npm run clean && NODE_ENV=production npm run build:es && NODE_ENV=production npm run build:lib --",
    "build:es": "tsc",
    "build:lib": "webpack",
    "server": "POST=8080 http-server build",
    "clean": "rimraf build",
    "tsc": "tsc --skipLibCheck --noEmit"
  },
  "keywords": [],
  "author": "brizy",
  "license": "MIT",
  "files": [
    "package.json",
    "package-lock.json",
    "build"
  ],
  "dependencies": {
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.20",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
