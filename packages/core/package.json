{
  "name": "@builder/core",
  "version": "1.0.0",
  "description": "Builder core",
  "main": "public/index.js",
  "module": "public/es/index.js",
  "types": "public/es/index.d.ts",
  "scripts": {
    "start": "concurrently --kill-others \"npm run build:es -- --watch\" \"npm run build:lib -- --watch\" \"npm run server\"",
    "build": "npm run clean && npm run build:es && npm run build:lib",
    "build:prod": "npm run clean && NODE_ENV=production npm run build:es && NODE_ENV=production npm run build:lib",
    "build:es": "tsc",
    "build:lib": "node bin/build.js",
    "server": "POST=8080 http-server public",
    "clean": "rimraf public/es/ public/lib/ public/index.js public/index.js.map",
    "tsc": "tsc --skipLibCheck --noEmit"
  },
  "keywords": [],
  "author": "brizy",
  "license": "MIT",
  "files": [
    "package.json",
    "package-lock.json",
    "public/es",
    "public/lib"
  ],
  "dependencies": {
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.15.15",
    "http-server": "^14.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  }
}
