{
  "name": "@brizy/merge-page-assets",
  "version": "1.1.1",
  "description": "Merge page assets compiled by Brizy compiler",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "brizy",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src",
    "tsc": "tsc --noEmit",
    "test": "jest test",
    "check": "npm run lint && npm run tsc && npm run test",
    "clean": "rimraf ./dist",
    "build": "npm run clean && webpack --config webpack.config.js && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run clean && npm run check && npm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "brizy",
    "merge",
    "assets"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^8.47.0",
    "rimraf": "^6.0.1",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@brizy/readers": "^1.0.3",
    "fp-utilities": "^1.1.4",
    "uuid": "^11.0.5"
  }
}
